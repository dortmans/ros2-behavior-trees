<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Repeat num_cycles="{num_hello}">
            <Sequence>
                <Action ID="Print" message="Hello World"/>
            </Sequence>
        </Repeat>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="Print">
            <input_port type="std::string" name="message"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

